{
  "name": "vinyl",
  "version": "0.0.1",
  "description": "Frontend for openrecord platform.",
  "main": "TODO",
  "scripts": {
    "clean": "rm -rf dist turntable/generated turntable/schema.graphql",
    "start": "npm run start:web & npm run start:api",
    "start:web": "webpack-dev-server --mode development",
    "start:api": "docker-compose -f turntable/docker-compose.yml up -d && npm run create-dist && npm run serve:api",
    "serve:api": "netlify-lambda serve --config turntable/webpack.config.js turntable/functions",
    "serve:debug": "NODE_ENV=development ENDPOINT='http://localhost:4466' npx --node-arg=--inspect-brk netlify-lambda serve --config webpack.config.js turntable/functions",
    "build": "npm run clean && npm run build:api && npm run build:web",
    "build:web": "webpack -c webpack.config.js --mode production",
    "build:api": "npm run codegen && npm run compile:api",
    "deploy:staging": "cd turntable; env-cmd ../env/.staging.env prisma deploy",
    "codegen": "cd turntable; get-graphql-schema $ENDPOINT > schema.graphql && gql codegen",
    "compile:api": "netlify-lambda build --config turntable/webpack.config.js turntable/functions",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "config": "node bin/printConfig.js",
    "create-dist": "mkdir -p dist/functions && touch dist/functions/main.js",
    "migrate": "node turntable/built/migrations/index.js",
    "toml": "babel-node scripts/create_netlify_env.js"
  },
